{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "Node",
    "outDir": "lib",
    "module": "ES6",
    "target": "ES5",
    "lib": ["DOM", "ES5", "ES6", "ESNext"],
    "declaration": true,
    "sourceMap": false,
    "pretty": true,
    "incremental": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    ".github",
    "dist",
    "lib",
  ]
}